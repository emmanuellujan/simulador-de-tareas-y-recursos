			<deviceId>dev0</deviceId>
			<schedulingAlgorithm>FCFS</schedulingAlgorithm>
			<quantum>5</quantum>
		</device>
		<device>
			<deviceId>dev1</deviceId>
			<schedulingAlgorithm>FCFS</schedulingAlgorithm>
			<quantum>-1</quantum>

<?xml version="1.0"?>
<!-- dev0 is the cpu -->
<!-- priority must be >= 0 -->
<!-- if quantum is not needed set it to -1 -->
<inputData>
	<description>
		Simple test for three processes executed in
		one divice with priority scheduling algorithm
		and a five times quantum.
		You can compare it with test_case_2.xml
	</description>
	<processes>
		<process>
			<processId>proc0</processId>
			<priority>0</priority>
			<compUnits>
				<compUnit>dev0</compUnit>
				<compUnit>dev0</compUnit>
				<compUnit>dev0</compUnit>
			</compUnits>
		</process>
		<process>
			<processId>proc1</processId>
			<priority>2</priority>
			<compUnits>
				<compUnit>dev0</compUnit>
				<compUnit>dev0</compUnit>
				<compUnit>dev0</compUnit>
			</compUnits>
		</process>
		<process>
			<processId>proc2</processId>
			<priority>1</priority>
			<compUnits>
				<compUnit>dev0</compUnit>
				<compUnit>dev0</compUnit>
				<compUnit>dev0</compUnit>
			</compUnits>
		</process>
	</processes>
	<devices>
		<device>
			<deviceId>dev0</deviceId>
			<schedulingAlgorithm>PrioritiesSA</schedulingAlgorithm>
			<quantum>5</quantum>
		</device>
	</devices>
</inputData>



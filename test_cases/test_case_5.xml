<?xml version="1.0"?>
<!-- dev0 is the cpu -->
<!-- priority must be >= 0 -->
<!-- if quantum is not needed set it to -1 -->
<inputData>
	<description>Simple test for two processes executed in two divices</description>
	<description>
		Simple test for two processes executed in
		two divices with FCFS scheduling algorithms.
		One of the process generate an interruption,
		this interruption is interrupted by another
		interruption.
	</description>
	<processes>
		<process>
			<processId>proc0</processId>
			<priority>0</priority>
			<compUnits>
				<compUnit>dev1</compUnit>
				<compUnit>int_dev0</compUnit>
				<compUnit>int_dev0</compUnit>
				<compUnit>int_dev0</compUnit>
			</compUnits>
		</process>
		<process>
			<processId>proc1</processId>
			<priority>1</priority>
			<compUnits>
				<compUnit>dev1</compUnit>
				<compUnit>int_dev0</compUnit>
				<compUnit>int_dev0</compUnit>
				<compUnit>int_dev0</compUnit>
			</compUnits>
		</process>
	</processes>
	<devices>
		<device>
			<deviceId>dev0</deviceId>
			<schedulingAlgorithm>FCFS</schedulingAlgorithm>
			<quantum>-1</quantum>
		</device>
		<device>
			<deviceId>dev1</deviceId>
			<schedulingAlgorithm>FCFS</schedulingAlgorithm>
			<quantum>-1</quantum>
		</device>
	</devices>
</inputData>


